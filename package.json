{
  "name": "jamstack-starter",
  "version": "0.0.1",
  "description": "\"A boilerplate starter for modern JAMStack applications with TypeScript, Jest, Emotion, React, MobX, Storybooks\"",
  "private": true,
  "author": "codewaseem <codewaseem@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean:dist": "rm -rf dist",
    "build:prod": "yarn clean:dist && webpack --mode=production",
    "build:dev": "yarn clean:dist && webpack --mode=development",
    "build": "yarn build:prod"
  },
  "devDependencies": {
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
